{
    "grammar": {
        "nonterminals": [
            "F"
        ],
        "terminals": [
            "+",
            "-"
        ],
        "rules": [
            [
                "F",
                "F[+F]F[-F]F"
            ]
        ],
        "axiom": "F"
    },
    "mapping": {
        "F": "FORWARD",
        "+": "RIGHT",
        "-": "LEFT",
        "[": "SAVE",
        "]": "LOAD"
    },
    "config": {
        "segment_length": 200,
        "length_reduction": 0.5,
        "angle_offset": 25,
        "starting_state": {
            "x": 0,
            "y": -200,
            "angle": 90
        }
    }
}