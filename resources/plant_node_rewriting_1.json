{
    "grammar": {
        "nonterminals": [
            "F",
            "X"
        ],
        "terminals": [
            "+",
            "-"
        ],
        "rules": [
            [
                "X",
                "F[+X]F[-X]+X"
            ],
            [
                "F",
                "FF"
            ]
        ],
        "axiom": "X"
    },
    "mapping": {
        "F": "FORWARD",
        "X": "FORWARD",
        "+": "RIGHT",
        "-": "LEFT",
        "[": "SAVE",
        "]": "LOAD"
    },
    "config": {
        "segment_length": 400,
        "length_reduction": 0.5,
        "angle_offset": 20,
        "starting_state": {
            "x": 0,
            "y": -400,
            "angle": 90
        }
    }
}